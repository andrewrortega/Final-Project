<div class="page-header">
  <h3><%= current_user.first_name %>'s Profile</h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Username</dt>
      <dd><%= current_user.username %></dd>

      <dt>First Name</dt>
      <dd><%= current_user.first_name %></dd>

      <dt>Last Name</dt>
      <dd><%= current_user.last_name %></dd>

      <dt>Dog Name</dt>
      <dd><%= current_user.dog_name %></dd>

      <dt>Dog Breed</dt>
      <dd><%= current_user.dog_breed %></dd>

      <dt>Location</dt>
      <dd><%= current_user.location %></dd>

      <dt>Email</dt>
      <dd><%= current_user.email %></dd>


    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/" class="btn btn-primary">
        Back
      </a>
    </div>
  </div>
</div>
