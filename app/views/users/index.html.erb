<div class="page-header">
  <h1>
  Users
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Username</th>
        <th>First_Name</th>
        <th>Last_Name</th>
        <th>Dog_Name</th>
        <th>Dog_Breed</th>
        <th>Location</th>
        <th>Email</th>

      </tr>

      <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.dog_name %></td>
        <td><%= user.dog_breed %></td>
        <td><%= user.location %></td>
        <td><%= user.email %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
